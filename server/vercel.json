{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "swagger.yaml",
          "node_modules/swagger-ui-dist/**",
          "node_modules/swagger-ui-express/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/server.ts" }
  ]
}